  box-shadow: 2px 2px 3px #e8e8e8;
  -moz-border-radius: 4px;
  border-radius: 4px;
  width: 300px;
  padding: 15px;
  background-image: url(/pub/TWiki/ScrollBoxAddOn/gradient-title.png);
  background-repeat: repeat-x;
  background-color: #ffffff;
 }
