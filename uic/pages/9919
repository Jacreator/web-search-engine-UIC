  text-align:center;
  font-size:19px;
  font-weight:bold;
  color: #333335;
  margin: -5px 0 0 0;
  padding: 0 0 12px 0;
  white-space: nowrap;
  overflow: hidden;
 }
 .scrollBoxTitle a:link,
 .scrollBoxTitle a:visited {
  color: #333335;
 }
 .scrollBoxContainer {
  overflow: hidden;
  height: 40px;
  padding: 0px;
  position: relative;
 }
