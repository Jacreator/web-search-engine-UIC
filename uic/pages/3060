<script>
    $(function() {
        if (window.jsmap) {
            jsmap('#map', {"flat": "simple", "max_zoom": 17, "agencies": [1063], "show_speed": false, "min_zoom": 10, "announcements": true, "stop_popup_mode": "simple", "route_default_on": true, "bounds": [41.866789, -87.675524, 41.87543, -87.633347], "label": "none", "load_indicator": "all", "arrival_predictions": true, "feeds_base_url": "//feeds.transloc.com/3/", "sms_keyword": "UICBUS", "public_only": true});
        } else {
            $('#map').hide();
            $('#broken-map-warning').show();
            if (window.Bugsnag) {
                window.Bugsnag.notify('jsmapUndefined', false, {}, 'warning');
            }
        }
    });
</script>



