		<SCRIPT language="JavaScript" type="text/javascript">

		

		var cookieEnabled=(navigator.cookieEnabled)? true : false



		//if not IE4+ nor NS6+

		if (typeof navigator.cookieEnabled=="undefined" && !cookieEnabled){ 

		document.cookie="testcookie"

		cookieEnabled=(document.cookie.indexOf("testcookie")!=-1)? true : false

		}

		if (!cookieEnabled){

		alert("YOU MUST HAVE COOKIES ENABLED TO SUBMIT GIFTS");

		}

		

		

// ---------------------------------------------------
// My_Form_Validator()- Client side form validator 
// ---------------------------------------------------

function submitme() {

	var vform;

	vform = document.Form1;

	if (My_Form_Validator() == false)
		return false;

	vform.action = vform.elements['server'].value

	vform.submit();

	return true;
}

function validatefreeformfundnameentry(amountsource,fundsource,hiddenfield) {

	var vform;

	vform = document.Form1;

	if ((vform.elements[amountsource].value != 0) && ((vform.elements[fundsource].value.length < 2) || (vform.elements[fundsource].value == "Indicate where to direct donation here"))){
		alert("Please enter a fund to receive your donation.");
		vform.elements[fundsource].focus();
		return false;
	}
	else {
		vform.elements[hiddenfield].value = "00000000/" + vform.elements[fundsource].value;

		//alert( vform.elements[fundsource].value.toString());
		//alert(vform.elements[hiddenfield].value.toString());

	}

}

function validatedropdownfunds(amountsource,fundsource) {

	var vform;
	var noselection = "OTHER1";
	var alertmessage = "Please select a fund to receive your donation."

	vform = document.Form1

	if ((vform.elements[amountsource].value != 0) && (vform.elements[fundsource][vform.elements[fundsource].selectedIndex].value == noselection)) {
		alert(alertmessage);
		vform.elements[fundsource].focus();
		return false;
	}
}

function My_Form_Validator(){

	var vform;

	vform = document.Form1;

	if (vform.elements["totalamount"].value == 0){
		alert("Please enter a donation to at least one fund before submitting.");
		return false;
	}
	
/* COMMENTED OUT - not drop down fields
	if (validatedropdownfunds("GIFT_AMOUNT11","FUND11") == false)
		return false;

	if (validatedropdownfunds("GIFT_AMOUNT12","FUND12") == false)
		return false;

	if (validatedropdownfunds("GIFT_AMOUNT1","FUND1") == false)
		return false;

	if (validatedropdownfunds("GIFT_AMOUNT10","FUND10") == false)
		return false;

	if (validatedropdownfunds("GIFT_AMOUNT2","FUND2") == false)
		return false;
*/

	if (validatedropdownfunds("GIFT_AMOUNT3","FUND3") == false)
		return false; 

	if (validatedropdownfunds("GIFT_AMOUNT4","FUND4") == false)
		return false; 

	if (validatedropdownfunds("GIFT_AMOUNT5","FUND5") == false)
		return false; 

	
/* COMMENTED OUT ON 6/9/15 PER MICHAEL WESBECHER RT TICKET #80217
	if (validatedropdownfunds("GIFT_AMOUNT6","FUND6") == false)
		return false; 
*/

	if (validatefreeformfundnameentry("GIFT_AMOUNT7","fundEntry7","FUND7") == false)
		return false;

/*	if (validatefreeformfundnameentry("GIFT_AMOUNT8","fundEntry8","FUND8") == false)
		return false;

