<script type="text/javascript">
jQuery(document).ready(function () {
        /// find the switch class and get rid of it
		jQuery(".switch").html('');
		jQuery(".switch").removeClass("switch");
        var tree_id = 0;
      /*  jQuery('div.kbe_category:has(.kbe_child_category)').addClass('has-child').prepend('<span class="switch"><img src="/kbe_images/kbe_icon-plus.png" /> Expand Subcategories</span>').each(function () {
            tree_id++;
            jQuery(this).attr('id', 'tree' + tree_id);
        }); */
		
		jQuery('div.kbe_category:has(.kbe_child_category)').addClass('has-child').each(function () {
            tree_id++;
            jQuery(this).attr('id', 'tree' + tree_id);
        });
		
		jQuery('div.kbe_category:has(.kbe_child_category) > h2').after('<span class="switch"><img src="http://mentalhealthrecovery.com/wp-content/themes/canvas/kbe_images/kbe_icon-plus-green_light.png" /> Expand Subcategories</span>');

        jQuery('div.kbe_category > span.switch').click(function () {
            var tree_id = jQuery(this).parent().attr('id');
            if (jQuery(this).hasClass('open')) {
                jQuery(this).parent().find('div:first').slideUp('fast');
                jQuery(this).removeClass('open');
                jQuery(this).html('<img src="http://mentalhealthrecovery.com/wp-content/themes/canvas/kbe_images/kbe_icon-plus-green_light.png" /> Expand Subcategories');
            } else {
                jQuery(this).parent().find('div:first').slideDown('fast');
                jQuery(this).html('<img src="http://mentalhealthrecovery.com/wp-content/themes/canvas/kbe_images/kbe_icon-minus_green_light.png" /> Collapse Subcategories');
                jQuery(this).addClass('open');
            }
        });
    });
</script>
       
    

    
